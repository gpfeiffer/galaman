<h1>Listing users</h1>
<% if notice %>
  <p id="notice"><%= notice %></p>
<% end %>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.name, user %></td>
    <td><%= user.email %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
