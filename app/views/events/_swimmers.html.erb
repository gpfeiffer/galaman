<h3>Heat 1</h3>

<table>
<tr>
<th>Name</th>
<th>Age</th>
<th>Team</th>
<th>Seed Time</th>
<th>Result</th>
</tr>
<% @event.entries.each do |entry| %>
  <tr>
      <td><%= link_to entry.swimmer.name, entry.swimmer %></td>
      <td align="right"><%= entry.swimmer.age(@event.competition.date) %></td>
      <td><%= entry.swimmer.club.symbol %></td>
      <td align="right"><%= entry %></td>
      <td align="right">
	<% if entry.result %>
	  <%= link_to entry.result, entry.result %>
	<% else %>
	  <%= link_to '__:__.__', new_result_path(:entry_id => entry.id) %>
        <% end %>
      </td>
  </tr>
<% end %>
</table>

<br />

