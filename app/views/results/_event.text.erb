<%= '=' * 54 %>
Event <%= event.pos %>. <%= event.discipline.to_words %>
<%= '-' * 54 %>
<% event.qualification_age_ranges.sort_by(&:min).each do |ages| %>
<% results = event.listed_results(ages) %>
<% if not results.empty? %>
<%= ages_to_words(ages) %>
<%= '-' * 54 %>
<%= sprintf("%4s %-25s %-6s %9s", "Plc",  "Name, Age", "Team", "Result") %>
<%= '-' * 54 %>
<%= render results %>
<% end %>
<% end %>
