<% if @event %>
<%= render :partial => 'event', :object => @event %>
<% elsif @invitation %>
<%= render :partial => 'invitation', :object => @invitation %>
<% elsif @competition %>
<%= render :partial => 'competition', :object => @competition %>
<% elsif @swimmer and @discipline %>
<%= render :partial => 'swimmer', :object => @swimmer, :discipline => @discipline %>
<% else %>
<%= render @results %>
<% end %>
