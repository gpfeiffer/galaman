<tr class="fact-major">
    <td><%= entry.heat.pos if entry.heat %></td>
    <td><%= entry.lane %></td>
    <td><%= link_to entry.name, entry.subject %></td>
    <td align="right"><%= entry.age %></td>
    <td><%= entry.club.symbol %></td>
    <td align="right"><%= entry %></td>
    <td align="right">
      <% if entry.result %>
        <%= link_to entry.result, entry.result %>
      <% else %>
        <%= link_to '__:__.__', new_result_path(:entry_id => entry.id) %>
      <% end %>
    </td>
</tr>

